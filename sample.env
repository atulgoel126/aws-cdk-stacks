# Required for all stacks
CDK_DEFAULT_ACCOUNT=<AWS Account ID>
CDK_DEFAULT_REGION=<AWS Region, e.g. ap-southeast-1>

# Required for CICD-* and MultiArchPipeline stacks
CDK_GITHUB_CONNECTION_ARN=<Go to https://console.aws.amazon.com/codesuite/settings/connections to set up a connection to GitHub and paste the ARN here, e.g. arn:aws:codestar-connections:ap-southeast-1:111222333444:connection/a-b-c-d-e
CDK_GITHUB_OWNER=<GitHub Username, e.g. tchangkiat>
CDK_GITHUB_REPO=<GitHub repository which contains an application with Dockerfile, e.g. sample-express-api>
CDK_GITHUB_REPO2=<GitHub repository which contains an index.html file and appspec.yml, e.g. sample-html>